@model List<ApplicationCore.Models.MovieCardResponseModel>

@{
    ViewData["Title"] = "Home Page";
}

<div class="rounded">
    <div class="container-fluid">
        <div class="row">
            @foreach(var movie in Model)
            {
                <div class="col-6">
                    @await Html.PartialAsync("_MovieCard", movie);
                </div>
            }
        </div>
    </div>
</div>
